<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="3dp"
                android:color="#FF919191" />

            <solid android:color="#FFDDDDDD" />
            <corners android:radius="10dp"/>
        </shape>
    </item>
</selector>